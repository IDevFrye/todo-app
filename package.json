{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "TodoApp — это простое веб-приложение для управления задачами. Оно позволяет добавлять, редактировать, удалять и завершать задачи, а также сохранять их в Local Storage для каждого пользователя.",
  "main": "index.js",
  "scripts": {
    "start": "npm install&&set NODE_ENV=development&&webpack serve --open",
    "dev": "set NODE_ENV=development&&webpack",
    "build": "set NODE_ENV=production&&webpack",
    "clear": "rd /s /q dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1"
  }
}
